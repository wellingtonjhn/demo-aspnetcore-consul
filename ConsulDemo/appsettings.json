{
  "ServiceDiscovery": {
    "ServiceName": "ConsulDemoService",
    "Endpoints": [],
    "HealthCheck": {
      "HealthCheckEndpoint": "/healthcheck",
      "DeregisterCriticalServiceAfterMinutes": 1,
      "CheckIntervalSeconds": 5
    },
    "Consul": {
      "HttpEndpoint": "http://consul:8500",
      "DnsEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  }
}
